{
  "name": "remote-lib-buildable",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/remote-lib-buildable/src",
  "prefix": "ng-mfa",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/remote-lib-buildable/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/remote-lib-buildable/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/remote-lib-buildable/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/remote-lib-buildable/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/remote-lib-buildable/**/*.ts",
          "libs/remote-lib-buildable/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
